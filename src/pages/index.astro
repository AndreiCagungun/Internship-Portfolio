
---
import Navbar from '../components/Navbar.astro';
import Footer from '../components/Footer.astro';
---

<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <meta content="width=device-width, initial-scale=1.0" name="viewport">
  <title>Home</title>
  <meta name="description" content="">
  <meta name="keywords" content="">

  <!-- Favicons -->
  <link href="/assets/img/logo.png" rel="icon">
  <link href="/assets/img/apple-touch-icon.png" rel="apple-touch-icon">

  <!-- Fonts -->
  <link href="https://fonts.googleapis.com" rel="preconnect">
  <link href="https://fonts.gstatic.com" rel="preconnect" crossorigin>
  <link href="https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,100;0,300;0,400;0,500;0,700;0,900;1,100;1,300;1,400;1,500;1,700;1,900&family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap" rel="stylesheet">

  <!-- Vendor CSS Files -->
  <link href="/assets/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
  <link href="/assets/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
  <link href="/assets/vendor/aos/aos.css" rel="stylesheet">
  <link href="/assets/vendor/glightbox/css/glightbox.min.css" rel="stylesheet">
  <link href="/assets/vendor/swiper/swiper-bundle.min.css" rel="stylesheet">

  <!-- Main CSS File -->
  <link href="/assets/css/main.css" rel="stylesheet">
</head>

<body class="index-page">


      <Navbar />


  <main class="main">
    <!-- Hero Section -->
    <section id="hero" class="hero section">
      <img src="/assets/img/hero-bg-abstract.jpg" alt="" data-aos="fade-in" class="">
      <div class="container">
        <div class="row justify-content-center" data-aos="zoom-out">
          <div class="col-xl-7 col-lg-9 text-center">
            <h1>AI Internship Projects</h1>
            <p>These are all the projects we’ve successfully completed during our entire OJT period, showcasing our dedication and hard work over the hours and days invested.</p>
          </div>
        </div>
        <div class="text-center" data-aos="zoom-out" data-aos-delay="100">
          <a href="/projects" class="btn-explore">Go to Projects</a>
        </div>
      </div>
    </section><!-- /Hero Section -->

    <!-- Portfolio Section -->
    <section id="portfolio" class="portfolio section">
      <div class="container section-title" data-aos="fade-up">
        <h2>Portfolio</h2>
        <p>These are all the projects we’ve successfully completed during our entire OJT period, showcasing our dedication and hard work over the hours and days invested.</p>
      </div>
      <div class="container">
        <div class="isotope-layout" data-default-filter="*" data-layout="masonry" data-sort="original-order">
          <div class="row gy-4 isotope-container" data-aos="fade-up" data-aos-delay="200">
            <div class="col-lg-4 col-md-6 portfolio-item isotope-item filter-app">
              <img src="/assets/img/dify/Dify.png" class="img-fluid" alt="">
              <div class="portfolio-info">
                <h4>Dify</h4>
                <p><a href="/dify">Learn more</a></p>
              </div>
            </div><!-- End Portfolio Item -->
            <div class="col-lg-4 col-md-6 portfolio-item isotope-item filter-app">
              <img src="/assets/img/langchain/lang.png" class="img-fluid" alt="">
              <div class="portfolio-info">
                <h4>LangChain</h4>
                <p><a href="/lang">Learn more</a></p>
              </div>
            </div><!-- End Portfolio Item -->
            <div class="col-lg-4 col-md-6 portfolio-item isotope-item filter-product">
              <img src="/assets/img/divoom/divoom.jpeg" class="img-fluid" alt="">
              <div class="portfolio-info">
                <h4>Divoom</h4>
                <p><a href="/divoom">Learn more</a></p>
              </div>
            </div><!-- End Portfolio Item -->
          </div><!-- End Portfolio Container -->
        </div>
      </div>
      <div class="text-center" data-aos="zoom-out" data-aos-delay="100" id="hero" class="hero section">
        <h2 style="font-size: 32px; font-weight: 700; margin-bottom: 15px; text-transform: uppercase; padding-top: 20px;">Personal Projects</h2>
        <p>A collection of innovative projects showcasing the unique skills and passions of our Cloudstaff interns.</p>
        <a href="/personalProjects" class="btn-explore">Explore</a>
      </div>
    </section><!-- /Portfolio Section -->
  </main>

  <Footer />

  <!-- Scroll Top -->
  <a href="#" id="scroll-top" class="scroll-top d-flex align-items-center justify-content-center"><i class="bi bi-arrow-up-short"></i></a>

  <!-- Preloader -->
  <div id="preloader"></div>

  <!-- Vendor JS Files -->
  <script is:inline src="/assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
  <script is:inline src="/assets/vendor/php-email-form/validate.js"></script>
  <script is:inline src="/assets/vendor/aos/aos.js"></script>
  <script is:inline src="/assets/vendor/purecounter/purecounter_vanilla.js"></script>
  <script is:inline src="/assets/vendor/glightbox/js/glightbox.min.js"></script>
  <script is:inline src="/assets/vendor/swiper/swiper-bundle.min.js"></script>
  <script is:inline src="/assets/vendor/imagesloaded/imagesloaded.pkgd.min.js"></script>
  <script is:inline src="/assets/vendor/isotope-layout/isotope.pkgd.min.js"></script>

  <!-- Main JS File -->
  <script is:inline src="/assets/js/main.js"></script>
</body>
</html>